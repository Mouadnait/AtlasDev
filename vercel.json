{
    "version" : 2,
    "builds" : [
        {
            "src" : "./frontend/src/App.jsx",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src" : "/(.*)",
            "dest" : "./frontend/src/App.jsx",
            "methods" : ["GET", "POST","PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers" : {
                "Access-Control-Allow-Origin" : "*"
            }
        }
    ]
}